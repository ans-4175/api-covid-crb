{
  "version": 2,
  "builds": [
  	{ "src": "lambdas/index.js", "use": "@now/node" },
  	{ "src": "lambdas/crawl.js", "use": "@now/node" },
  	{ "src": "lambdas/check-data.js", "use": "@now/node" },
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/lambdas/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/crawl",
      "dest": "/lambdas/crawl.js",
      "methods": ["GET"]
    },
    {
      "src": "/check-data",
      "dest": "/lambdas/check-data.js",
      "methods": ["GET"]
    },
  ]
}